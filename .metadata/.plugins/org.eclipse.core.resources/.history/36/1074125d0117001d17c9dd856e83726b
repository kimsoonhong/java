

public class AccountingApp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		System.out.println("Value of supply :"+12345.9);
		System.out.println("VAT :"+ (12345.9*0.1));
		System.out.println("Total :"+ (12345.9+12345.9*0.1));
		System.out.println("Expense :"+ (12345.9*0.3));
		System.out.println("Expense :"+ (12345.9*0.3));
		System.out.println("Income :"+ (12345.9-12345.9*0.3));
		System.out.println("Dividend 1 :"+ (12345.9-12345.9*0.3)*0.5);
		System.out.println("Dividend 2 :"+ (12345.9-12345.9*0.3)*0.3);
		System.out.println("Dividend 3 :"+ (12345.9-12345.9*0.3)*0.2);
		
		
	}

}
